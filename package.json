{
  "name": "graph-flow",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "watch:wasm": "cargo watch -C ./wasm-rs -s \"pnpm build:wasm\"",
    "check:front": "vue-tsc --noEmit",
    "check:wasm": "cargo check --manifest-path ./wasm-rs/Cargo.toml",
    "build": "run-p check:* build:wasm build:front",
    "build:front": "vite build",
    "build:wasm": "wasm-pack build ./wasm-rs --target web",
    "preview": "vite preview --port 4173",
    "test": "run-p test:front",
    "test:front": "vitest --environment jsdom run",
    "test:coverage": "vitest --environment jsdom run --coverage",
    "test:watch": "vitest --environment jsdom",
    "test:ui": "vitest --environment jsdom --ui",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "test:wasm": "wasm-pack test --chrome ./wasm-rs",
    "benchmark": "ts-node benchmark",
    "lint": "run-p lint:*",
    "lint:script": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:css": "prettier --write \"src/**/*.{less,postcss,css,scss}\"",
    "lint:wasm": "cargo +nightly fmt --manifest-path ./wasm-rs/Cargo.toml",
    "prepare": "husky install"
  },
  "dependencies": {
    "wasm-rs": "./wasm-rs/pkg",
    "@vueuse/core": "^9.1.1",
    "@vueuse/integrations": "^9.1.1",
    "dayjs": "^1.11.5",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.20",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/types": "^17.0.0",
    "@iconify/json": "^2.1.97",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^16.2.15",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/ui": "^0.21.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "benny": "^3.7.1",
    "c8": "^7.12.0",
    "cypress": "^10.6.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1",
    "rollup-plugin-visualizer": "^5.8.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "terser": "^5.15.0",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vitest": "^0.18.1",
    "vue-tsc": "^0.38.9"
  }
}
